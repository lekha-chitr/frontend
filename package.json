		"@types/react": "16.7.13",
		"@types/react-dom": "16.0.11",
		"husky": "^1.2.0",
		"lint-staged": "^8.1.0",
		"prettier": "^1.15.3",
		"react": "^16.6.3",
		"react-dom": "^16.6.3",
		"react-scripts": "2.1.1",
		"styled-components": "^4.1.2",
